{
  "stryker-config": {
    "project-info": {
      "name": "github.com/lyndychivs/Palindrome",
      "module": "Palindrome",
      "version": "main"
    },
    "concurrency": 2,
    "mutation-level": "Complete",
    "language-version": "latest",
    "additional-timeout": 5000,
    "mutate": [
      "**/*"
    ],
    "solution": "Palindrome.sln",
    "configuration": "Debug",
    "target-framework": null,
    "project": "/Palindrome/Palindrome.csproj",
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "disable-mix-mutants": false,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "verbosity": "info",
    "reporters": [
      "Progress",
      "Html"
    ],
    "since": {
      "enabled": false,
      "ignore-changes-in": [],
      "target": "main"
    },
    "baseline": {
      "enabled": false,
      "provider": "disk",
      "azure-fileshare-url": "",
      "fallback-version": "main"
    },
    "dashboard-url": "https://dashboard.stryker-mutator.io",
    "test-projects": ["/Palindrome.Tests/Palindrome.Tests.csproj"],
    "test-case-filter": "",
    "ignore-mutations": [],
    "ignore-methods": [],
    "report-file-name": "mutation-report.html",
    "break-on-initial-test-failure": false
  }
}